{
  "name": "antdv-admin-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve --mode development",
    "dev:prod": "vue-cli-service serve --mode production",
    "build:dev": "vue-cli-service build --mode development --dest dev",
    "build:prod": "vue-cli-service build --mode production --dest prod",
    "build:all": "npm run build:dev && npm run build:prod",
    "preview": "serve -s prod",
    "preview:dev": "vue-cli-service build --mode development --dest preview-dev && serve -s preview-dev",
    "preview:prod": "vue-cli-service build --mode production --dest preview-prod && serve -s preview-prod",
    "lint": "vue-cli-service lint",
    "cz": "npm run log && git add . && git cz",
    "log": "conventional-changelog --config ./node_modules/vue-cli-plugin-commitlint/lib/log -i CHANGELOG.md -s -r 0"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/runtime": "^7.11.2",
    "ant-design-vue": "^1.6.5",
    "axios": "^0.20.0",
    "core-js": "^3.6.5",
    "install": "^0.13.0",
    "less-loader": "^5.0.0",
    "moment": "^2.28.0",
    "monaco-editor": "^0.21.2",
    "normalize.css": "^8.0.1",
    "npm": "^6.14.8",
    "nprogress": "^0.2.0",
    "secure-ls": "^1.2.6",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0",
    "vuex-persistedstate": "^3.1.0",
    "vxe-table": "^2.9.25",
    "xe-utils": "^2.7.14"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "commitizen": "^4.0.3",
    "commitlint": "^8.2.0",
    "conventional-changelog-cli": "^2.0.28",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^3.0.9",
    "less": "^3.12.2",
    "mockjs": "^1.1.0",
    "monaco-editor-webpack-plugin": "^2.0.0",
    "right-pad": "^1.0.1",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "serve": "^11.3.2",
    "svg-sprite-loader": "^5.0.0",
    "svgo": "^1.3.2",
    "svgo-loader": "^2.2.1",
    "vue-cli-plugin-commitlint": "~1.0.12",
    "vue-template-compiler": "^2.6.11"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/vue-cli-plugin-commitlint/lib/cz"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}